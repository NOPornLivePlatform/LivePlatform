/* background */
QWidget#centralWidget
{
	background: rgba(36,41,46,0.9);
	background-position: top;
	background-image: url(:/LiveLoginDialog/res/login-background-600x180.png);
	background-repeat: no-repeat;
}

QLabel#applicationIconLabel
