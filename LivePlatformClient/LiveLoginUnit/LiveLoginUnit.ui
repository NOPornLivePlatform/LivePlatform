}
/* avatar */
QLabel#avatarLabel
{
	border-image: url(:/LiveLoginUnit/res/avatar-mask.png);
	border-radius:60px;
	min-width:  120px;
    max-width:  120px;
    min-height: 120px;
    max-height: 120px;
    border-width: 0 0 0 0;
}

/* button */
QPushButton,QCheckBox,QToolButton
{
	color: white;
	background-color: transparent;
}
QPushButton:hover, QCheckBox:hover
{
	color: yellow;
}
QPushButton:pressed, QCheckBox:pressed
{
	color: #FFCC66;
}
QPushButton:focus, QCheckBox:focus
{
	outline: none
}
/* login button */
QPushButton#loginPushButton {
  background: #0078d7;
  border: 2px solid rgba(0,0,0,0.4)
}
QPushButton#loginPushButton:hover {
  background: #1CA8FA;
}
QPushButton#loginPushButton:pressed {
  background: #2A67FF;
}
/* account password */
QLineEdit#passwordLineEdit,QLineEdit#accountComboBox
{
	border: 2px solid rgba(27,128,255,0.5);
	background: rgba(45,64,72,0.8);
	color: white;
    selection-background-color:green;
}
QLineEdit#passwordLineEdit:focus,QLineEdit#accountComboBox:focus
{
	background: rgba(80,100,110,0.8);
}
/* tooltip */
QToolTip
{
	color: yellow;
	border: 2px solid #333;
	background-color: black;
}</string>
